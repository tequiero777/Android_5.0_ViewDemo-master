<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.databinding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.BasicActivity" />
        <activity android:name=".ui.AliasActivity" />
        <activity android:name=".ui.StaticMethodActivity" />
        <activity android:name=".ui.ResourceActivity" />
        <activity android:name=".ui.CustomNameActivity" />
        <activity android:name=".ui.ObservableActivity" />
        <activity android:name=".ui.ViewIdActivity" />
        <activity android:name=".ui.ViewStubActivity" />
        <activity android:name=".ui.ConverterActivity" />
        <activity android:name=".ui.IncludeActivity" />
        <activity android:name=".ui.AttrSetActivity" />
        <activity android:name=".ui.DynamicActivity" />
        <activity android:name=".ui.NewsActivity" />
    </application>

</manifest>